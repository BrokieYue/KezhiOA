@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<script>
    var keyValue = $.request("keyValue");
    $(function () {
        $.ajax({
            url: "/StationManage/LineArea/GetFormJson",
            data: { keyValue: keyValue },
            dataType: "json",
            async: false,
            success: function (data) {
                $("#form1").formSerialize(data);
                $("#form1").find('.form-control,select,input').attr('readonly', 'readonly');
                $("#form1").find('div.ckbox label').attr('for', '');
            }
        });
    });
</script>

<form id="form1">
    <div style="padding-top: 20px; margin-right: 20px;">
        <table class="form">
           <tr>
                    <th class="formTitle">工厂代码</th>
                    <td class="formValue">
                        <input id="FactoryCode" name="FactoryCode" type="text" class="form-control required"/>
                    </td>
                    <th class="formTitle">生产线</th>
                    <td class="formValue">
                        <input id="LineCode" name="LineCode" type="text" class="form-control required"/>
                    </td>
                </tr>
                 <tr>
                    <th class="formTitle">区域编码</th>
                    <td class="formValue">
                        <input id="AreaCode" name="AreaCode" type="text" class="form-control required"/>
                    </td>
                    <th class="formTitle">区域名称</th>
                    <td class="formValue">
                        <input id="AreaName" name="AreaName" type="text" class="form-control required"/>
                    </td>
                </tr>
              <tr>
                    <th class="formTitle">工艺类型</th>
                    <td class="formValue">
                        <input id="ProductionTechType" name="ProductionTechType" type="text" class="form-control"/>
                    </td>
                </tr>
        </table>
    </div>
</form>
